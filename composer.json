{
    "name": "needlaravelsite/filament-otp-input",
    "description": "Filament otp input",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Needlaravelsite\\FilamentOtpInput\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Muhammad",
            "email": "m.nomaniu@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0|^5.0",
        "illuminate/contracts": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload-dev": {
        "psr-4": {
            "NeedLaravelSite\\FilamentOtpInput\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NeedLaravelSite\\FilamentOtpInput\\FilamentOtpInputServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
